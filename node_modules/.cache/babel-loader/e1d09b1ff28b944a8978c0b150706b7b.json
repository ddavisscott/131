an id=\"middle\">:</span>\n        <span id=\"seconds\">{{ seconds }}</span>\n    </div>\n\n    <!--  THE BUTTONS  -->\n<div id=\"buttons\">\n\n<!--  START BUTTON    -->\n<div id=\"buttons\">\n<!--     Start TImer -->\n<v-btn \ncolor=\"warning\"\nid=\"start\" \nclass=\"button is-dark is-large\" \n@click=\"startTimer\"\n>Start</v-btn>\n    <!--     Pause Timer -->\n    <v-btn \n    color=\"warning\"\n    id=\"stop\" \n    class=\"button is-dark is-large\"  \n    @click=\"stopTimer\">\n    Stop</v-btn>\n    <!--     Restart Timer -->\n   <v-btn \n    color=\"warning\"\n    id=\"reset\" \n    class=\"button is-dark is-large\" \n    @click=\"resetTimer\">\n    Reset</v-btn>\n</div>\n  </div>\n\n<!-- The bulk of the timer -->\n\n</div>\n</div>\n</section>\n</template>\n\n\n<style scoped>\n#message {\n  color: #DDD;\n  font-size: 50px;\n  margin-bottom: 20px;\n}\n\n#timer {\n  font-size: 40px;\n  line-height: 1;\n  margin-bottom: 40px;\n}\n.button{\n    background-color: red;\n}\n\n</style>\n\n\n<script>\nmodule.exports = {\n  data: function () {\n    return {\n      timer: null,\ntotalTime: (15 * 60),\nresetButton: false,\ntitle: \"Let the countdown begin!!\"\n    }\n  },\n\n  methods: {\nstartTimer: function() {\n  this.timer = setInterval(() => this.countdown(), 1000);\n  this.resetButton = true;\n},\nstopTimer: function() {\n  clearInterval(this.timer);\n  this.timer = null;\n  this.resetButton = true;\n},\nresetTimer: function() {\n  this.totalTime = (15 * 60);\n  clearInterval(this.timer);\n  this.timer = null;\n  this.resetButton = false;\n},\npadTime: function(time) {\n  return (time < 10 ? '0' : '') + time;\n},\ncountdown: function() {\n  this.totalTime--;\n}\n},\ncomputed: {\n    minutes: function() {\n      const minutes = Math.floor(this.totalTime / 60);\n      return this.padTime(minutes);\n    },\n    seconds: function() {\n      const seconds = this.totalTime - (this.minutes * 60);\n      return this.padTime(seconds);\n    }\n  }\n}\n</script>\n"],"sourceRo